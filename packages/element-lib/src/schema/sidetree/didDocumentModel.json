{
  "$id": "/sidetree.didDocument",
  "type": "object",
  "properties": {
    "@context": {
      "type": "string",
      "required": true,
      "enum": ["https://w3id.org/did/v1"],
      "example": ["https://w3id.org/did/v1"]
    },
    "publicKey": {
      "type": "array",
      "required": true,
      "items": {
        "$ref": "sidetree.key"
      }
    },
    "authentication": {
      "type": "array",
      "required": false,
      "items": {
        "type": "string"
      }
    },
    "assertionMethod": {
      "type": "array",
      "required": false,
      "items": {
        "type": "string"
      }
    },
    "capabilityDelegation": {
      "type": "array",
      "required": false,
      "items": {
        "type": "string"
      }
    },
    "capabilityInvocation": {
      "type": "array",
      "required": false,
      "items": {
        "type": "string"
      }
    },
    "keyAgreement": {
      "type": "array",
      "required": false,
      "items": {
        "anyOf": [{ "type": "string" }, { "$ref": "sidetree.key" }]
      }
    }
  },
  "additionalProperties": false
}
